<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Save Our Ship 2</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="StargateMod_Stargate"]/comps/li[@Class="StargatesMod.CompProperties_Stargate"]</xpath>
					<value>
						<thingsExcludedFromVortex>
							<li>ShipHullTile</li>
							<li>ShipHullfoamTile</li>
						</thingsExcludedFromVortex>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="StargateMod_OrlinStargate"]/comps/li[@Class="StargatesMod.CompProperties_Stargate"]</xpath>
					<value>
						<thingsExcludedFromVortex>
							<li>ShipHullTile</li>
							<li>ShipHullfoamTile</li>
						</thingsExcludedFromVortex>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="StargateMod_AdvancedStargate"]/comps/li[@Class="StargatesMod.CompProperties_Stargate"]</xpath>
					<value>
						<thingsExcludedFromVortex>
							<li>ShipHullTile</li>
							<li>ShipHullfoamTile</li>
						</thingsExcludedFromVortex>
					</value>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>