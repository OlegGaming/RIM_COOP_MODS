<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Royalty</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>Defs/ResearchProjectDef[defName="Skyranger_Research"]</xpath>
			<value>
				<techprintCount>2</techprintCount>
				<techprintCommonality>1</techprintCommonality>
				<techprintMarketValue>1500</techprintMarketValue>
				<heldByFactionCategoryTags>
					<li>Empire</li>
					<li>Outlander</li> <!-- didn't have this one earlier -->
				</heldByFactionCategoryTags>
			</value>
		</match>
	</Operation>
</Patch>